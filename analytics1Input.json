{
    "RedshiftDestinationConfiguration": {
        "RoleARN": "arn:aws:iam::varAccountID:role/firehose_delivery_role",
        "ClusterJDBCURL": "jdbc:redshift://varRedshiftClusterEndpoint:5439/workshopdb",
        "CopyCommand": {
            "DataTableName": "analytics1",
            "DataTableColumns": "playerip, handle, email, uuid, playerid, country, useragent, datestamp, walletbalance, playerlevel, status",
            "CopyOptions": "format as json 'auto' dateformat 'YYYY-MM-DD HH:MI:SS+/-TZ'"
        },
        "Username": "wsuser",
        "Password": "wsPassword0",
        "RetryOptions": {
            "DurationInSeconds": 300
        },
        "S3Configuration": {
            "RoleARN": "arn:aws:iam::varAccountID:role/firehose_delivery_role",
            "BucketARN": "arn:aws:s3:::varBucketName",
            "Prefix": "analytics1",
            "BufferingHints": {
                "SizeInMBs": 5,
                "IntervalInSeconds": 180
            },
            "CompressionFormat": "UNCOMPRESSED",
            "EncryptionConfiguration": {
                "NoEncryptionConfig": "NoEncryption"
            },
            "CloudWatchLoggingOptions": {
                "Enabled": true,
                "LogGroupName": "/aws/kinesisfirehose/workshopAnalyticsFH",
                "LogStreamName": "S3Delivery"
            }
        },
        "ProcessingConfiguration": {
            "Enabled": false
        },
        "S3BackupMode": "Disabled",
        "CloudWatchLoggingOptions": {
            "Enabled": true,
            "LogGroupName": "/aws/kinesisfirehose/workshopAnalyticsFH",
            "LogStreamName": "RedshiftDelivery"
        }
    }
}